{
    "name": "cortex/foundation",
    "description": "The core foundation of Rinvex Cortex modular application architecture.",
    "type": "cortex-module",
    "keywords": [
        "rinvex",
        "cortex",
        "module",
        "laravel",
        "foundation",
        "architecture",
        "modular",
        "kernel",
        "heart",
        "core"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-foundation/issues",
        "source": "https://github.com/rinvex/cortex-foundation",
        "docs": "https://github.com/rinvex/cortex-foundation/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-foundation/contributors"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "barryvdh/laravel-snappy": "^0.4.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.0",
        "edvinaskrucas/notification": "dev-master",
        "felixkiss/uniquewith-validator": "^3.4.0",
        "fideloper/proxy": "^4.3.0",
        "fruitcake/laravel-cors": "^1.0.0",
        "geoip2/geoip2": "^2.10.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.0",
        "laravel/framework": "^7.0.0",
        "laravelcollective/html": "^6.1.0",
        "league/fractal": "^0.18.0",
        "lord/laroute": "^2.4.0",
        "mcamara/laravel-localization": "^1.5.0",
        "mariuzzo/laravel-js-localization": "^1.5.0",
        "proengsoft/laravel-jsvalidation": "^3.0.0",
        "rinvex/laravel-composer": "^4.0.0",
        "rinvex/laravel-menus": "^4.0.0",
        "rinvex/laravel-pages": "^4.0.0",
        "rinvex/laravel-support": "^4.0.0",
        "silber/bouncer": "v1.0.0-rc.8",
        "spatie/laravel-activitylog": "^3.13.0",
        "spatie/laravel-collection-macros": "^6.1.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-sitemap": "^5.7.0",
        "symfony/console": "^5.0.0",
        "symfony/finder": "^5.0.0",
        "symfony/http-kernel": "^5.0.0",
        "torann/geoip": "^1.2.0",
        "vinkla/hashids": "^8.1.0",
        "watson/validating": "^5.0.0",
        "yajra/laravel-datatables-buttons": "^4.9.0",
        "yajra/laravel-datatables-fractal": "^1.5.0",
        "yajra/laravel-datatables-html": "^4.23.0",
        "yajra/laravel-datatables-oracle": "^9.9.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.27.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Foundation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Foundation\\Providers\\BootServiceProvider",
                "Cortex\\Foundation\\Providers\\DiscoveryServiceProvider",
                "Cortex\\Foundation\\Providers\\FoundationServiceProvider",
                "Cortex\\Foundation\\Providers\\ConsoleSupportServiceProvider",
                "Cortex\\Foundation\\Overrides\\Lord\\Laroute\\LarouteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
